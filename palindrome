impl Solution {
    pub fn is_palindrome(x: i32) -> bool {
        let xs = x.to_string();
        let xsIt = xs.chars();
        let xsRevIt = xs.chars().rev();
        let mut k = 0;
        let max = xs.len() / 2;
        for (i, j) in xsIt.zip(xsRevIt) {
            if (i != j) {
                return false;
            }
            k += 1;
            if (k >= max) {
                break;
            }
        }
        return true;
    }
}
